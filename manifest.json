{
    "manifest_version": 2,
    "name": "My Cloud9",
    "description":"My Cloud9 IDE by DigiNow.it allows you to store and connect to any local, remote or c9.io Cloud9 servers.",
    "minimum_chrome_version": "23",
    "version": "1.0.0.0",
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "64": "icon_64.png",
        "128": "icon_128.png",
        "256": "icon_256.png"
    },
    "app": {
        "background": {
            "scripts": [
                "main.js"
            ]
        }
    },
    "permissions": [
        "webview",
        "storage"
    ]
}
